            <stringProp name="script">import org.apache.jmeter.services.FileServer;
import java.lang.*;

int titelsCount = Integer.parseInt(vars.get(&quot;ExtractTitles_matchNr&quot;));
String currentDirectory=FileServer.getFileServer().getBaseDir();
String csvPath=currentDirectory+&quot;\\Titles.csv&quot;;
vars.put(&quot;fileLocation&quot;,csvPath);

f=new FileOutputStream(vars.get(&quot;fileLocation&quot;),false);
p=new PrintStream(f);
for(int i=1;i&lt;=titelsCount;i++)
{
p.println(vars.get(&quot;ExtractTitles_&quot;+i));
}
p.close();
f.close();
</stringProp>
